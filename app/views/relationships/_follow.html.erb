     <% if users.exists? %>
  <% users.each do |user| %>
  <table class='table'>
        <thead>
            <tr>
              <th>image</th>
              <th>name</th>
              <th></th>
              <th></th>
              <th></th>
            </tr>
         </thead>

         <tbody>
            <tr>
              <td><%= attachment_image_tag(user, :profile_image, :fill, 100, 100, fallback: "no_image.jpg") %></td>
              <td><%= user.name %></td>
              <td>フォロー数: <%= user.followings.count %></td>
              <td>フォロワー数: <%= user.followers.count %></td>
              <td><%= link_to 'Show', user_path(user) %></td>
            </tr>
        </tbody>
   </table>
  <% end %>
<% else %>
  <p>ユーザーはいません</p>
<% end %>